<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="2" configfailurepolicy="continue" name="emulator Suite" parallel="tests">
    <listeners>
        <listener class-name="helpers.TestListeners"></listener>
    </listeners>
    <parameter name="deviceName" value="iPhone 14 Pro"></parameter>
    <parameter name="platformVersion" value="16.2"></parameter>
    <parameter name="platformName" value="ios"></parameter>
    <parameter name="virtualDevice" value="true"></parameter>
    <test name="iPhone 14 Pro">
        <classes>
            <class name="workshop.HomeDashboardIOS"/>
        </classes>
    </test>

</suite>



