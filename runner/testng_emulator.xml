<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="2" configfailurepolicy="continue" name="emulator Suite" parallel="tests">
    <listeners>
        <listener class-name="helpers.TestListeners"></listener>
    </listeners>
    <parameter name="deviceName" value="Pixel5"></parameter>
    <parameter name="platformVersion" value="11.0"></parameter>
    <parameter name="platformName" value="Android"></parameter>
    <parameter name="virtualDevice" value="true"></parameter>
    <test name="Pixel5">
        <classes>
            <class name="workshop.HomeDashboard"/>
        </classes>
    </test>

